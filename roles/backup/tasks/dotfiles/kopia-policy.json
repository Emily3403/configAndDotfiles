{
  "(global)": {
    "retention": {
      "keepLatest": 10,
      "keepHourly": 48,
      "keepDaily": 14,
      "keepWeekly": 8,
      "keepMonthly": 24,
      "keepAnnual": 10,
      "ignoreIdenticalSnapshots": false
    },
    "files": {
      "ignore": [
        "*.img",
        "*.qcow2",
        ".mypy_cache/",
        ".sync_*",
        "__pycache__/",
        "build/",
        "node_modules/",
        "target/"
      ],
      "ignoreDotFiles": [
        ".kopiaignore"
      ]
    },
    "errorHandling": {
      "ignoreFileErrors": false,
      "ignoreDirectoryErrors": false,
      "ignoreUnknownTypes": true
    },
    "scheduling": {
      "intervalSeconds": 3600,
      "runMissed": true
    },
    "compression": {
      "compressorName": "none"
    },
    "metadataCompression": {
      "compressorName": "zstd-fastest"
    },
    "splitter": {},
    "actions": {},
    "osSnapshots": {
      "volumeShadowCopy": {
        "enable": 0
      }
    },
    "logging": {
      "directories": {
        "snapshotted": 5,
        "ignored": 5
      },
      "entries": {
        "snapshotted": 0,
        "ignored": 5,
        "cacheHit": 0,
        "cacheMiss": 0
      }
    },
    "upload": {
      "maxParallelSnapshots": 2,
      "parallelUploadAboveSize": 2147483648
    }
  },
